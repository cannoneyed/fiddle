    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "minimist": "^1.2.0",
    "mobx-react-devtools": "^5.0.1",
    "noop-webpack-plugin": "^1.0.1",
    "prettier": "^1.7.0",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^4.0.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "source-map-support": "^0.4.15",
    "spectron": "^3.7.0",
    "style-loader": "^0.18.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "ts-jest": "^21.1.3",
    "ts-loader": "^2.3.7",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.1",
    "url-loader": "^0.5.8",
    "webpack": "3.4",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^2.1.1",
    "@types/storybook__react": "^3.0.8",
    "classnames": "^2.2.5",
    "fast-json-patch": "^2.0.6",
    "jquery": "^3.2.1",
    "mobx": "^4.2.0",
    "mobx-logger": "^0.7.0",
    "mobx-react": "^5.0.0",
    "mobx-utils": "^5.0.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-transition-group": "^2.3.0",
    "reflect-metadata": "^0.1.12",
    "stats.js": "^0.17.0",
    "styled-components": "^3.2.6",
    "tinycolor2": "^1.4.1",
    "tsconfig-paths": "^3.3.2",
    "typedi": "^0.7.2"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
