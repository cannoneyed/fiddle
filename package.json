    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^1.5.1",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "3",
    "fbjs-scripts": "^0.8.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.48.0",
    "flow-runtime": "^0.13.0",
    "flow-typed": "^2.1.2",
